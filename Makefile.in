@OPTION_DYNAMIC_LOAD_TRUE@                   -no-undefined    \
@OPTION_DYNAMIC_LOAD_TRUE@                   $(XSTATIC)       \
@OPTION_DYNAMIC_LOAD_TRUE@                   -export-dynamic  \
@OPTION_DYNAMIC_LOAD_TRUE@                   -avoid-version

